{
  "name": "ccd",
  "version": "3.0.2",
  "private": true,
  "type": "module",
  "author": "",
  "description": "现代化全局后台管理框架",
  "keywords": [
    "vue3",
    "typescript",
    "vite",
    "pinia",
    "alova",
    "admin",
    "framework",
    "ccd"
  ],
  "engines": {
    "node": ">=24.3.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "vite",
    "dev:icons": "NODE_OPTIONS='--max-old-space-size=8192' vite",
    "dev:demo": "UNO_DEMO=true vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:analyze": "vue-tsc --noEmit && vite build --mode analyze",
    "preview": "vite preview",
    "preview:dist": "pnpm build && pnpm dlx serve dist",
    "report": "npx vite-bundle-analyzer dist/stats.html",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write src/",
    "fix": "pnpm format && pnpm lint:fix",
    "check": "pnpm type-check && pnpm lint:check",
    "pre-commit": "pnpm check",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass,less,styl,json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pnpm": {
    "overrides": {
      "glob": "^11.0.3"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "vue-demi"
    ]
  },
  "dependencies": {
    "@primeuix/themes": "^2.0.3",
    "@primevue/forms": "^4.5.4",
    "@primevue/themes": "^4.5.4",
    "@vueuse/core": "^13.9.0",
    "@vvo/tzdb": "^6.183.0",
    "alova": "^3.3.3",
    "animate.css": "^4.1.1",
    "crypto-es": "^3.1.2",
    "date-holidays": "^3.26.1",
    "echarts": "^6.0.0",
    "lodash-es": "^4.17.21",
    "lz-string": "^1.5.0",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "overlayscrollbars": "^2.14.0",
    "overlayscrollbars-vue": "^0.5.9",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.7.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.4",
    "uuid": "^13.0.0",
    "vue": "^3.5.12",
    "vue-echarts": "^8.0.0",
    "vue-i18n": "^10.0.8",
    "vue-router": "^4.4.5",
    "xlsx": "^0.18.5",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^9.9.0",
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/lucide": "^1.2.86",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify/utils": "^3.1.0",
    "@primevue/auto-import-resolver": "^4.5.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.19.7",
    "@types/nprogress": "^0.2.3",
    "@types/postcss-pxtorem": "^6.1.0",
    "@unocss/preset-attributify": "^66.6.0",
    "@unocss/preset-icons": "^66.6.0",
    "@unocss/preset-typography": "^66.6.0",
    "@unocss/preset-uno": "^66.6.0",
    "@unocss/reset": "^66.6.0",
    "@unocss/transformer-directives": "^66.6.0",
    "@unocss/transformer-variant-group": "^66.6.0",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.23",
    "baseline-browser-mapping": "^2.9.19",
    "chokidar": "^4.0.3",
    "commitizen": "^4.3.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.13",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^9.31.0",
    "glob": "^11.1.0",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "npm-run-all2": "^7.0.2",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.6",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.8.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.97.3",
    "terser": "^5.46.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.53.1",
    "unocss": "^66.6.0",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.10"
  }
}
